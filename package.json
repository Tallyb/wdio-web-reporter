{
  "name": "wdio-web-reporter",
  "version": "0.1.1",
  "description": "A WebdriverIO reporter. Report results to a webhook.",
  "main": "build/reporter.js",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ../../babel.config.js",
    "test:only": "jest",
    "test": "npm run lint && npm run test:only",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "lint": "eslint src tests",
    "prerelease:*": "npm run build",
    "release:patch": "np patch --yolo --no-yarn",
    "release:minor": "np minor --yolo --no-yarn",
    "release:major": "np major --yolo --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/wdio-spec-reporter.git"
  },
  "directories": {
    "lib": "./src"
  },
  "keywords": [
    "spec",
    "reporter",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-reporter"
  ],
  "author": "Tally Barak <tally.barak@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/wdio-spec-reporter/issues"
  },
  "homepage": "https://github.com/webdriverio/wdio-spec-reporter#readme",
  "dependencies": {
    "sync-request": "^6.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.0.0",
    "jest": "^24.3.1",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "contributors": []
}
